{
  "buildCommand": "cd ../.. && pnpm run build --filter=@nextplate/api",
  "outputDirectory": ".",
  "installCommand": "cd ../.. && pnpm install --frozen-lockfile",
  "framework": null,
  "functions": {
    "api/index.ts": {
      "maxDuration": 10
    }
  },
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/api"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
